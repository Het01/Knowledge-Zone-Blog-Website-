<%- include('Partials/header'); %>

<h1>Home</h1>

<p><%= homeStartingContent %></p>
<form action="/" method="post">
    <button type="submit" name="submit" onClick={navigateCompose} class="btn btn-primary">Publish</button>
</form>
<% for(var i=0;i<posts.length ; i++){ %>
    <h1> <%= posts[i].title %></h1>
    <p> <%= posts[i].content.substring(0,100)+"..." %> <a href="post/<%= posts[i].title %>">Read more</a></p>
<% } %>

<%- include('Partials/footer'); %>